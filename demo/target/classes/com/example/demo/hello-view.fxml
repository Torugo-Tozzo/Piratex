<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="566.0" prefWidth="240.0" spacing="20.0" style="-fx-background-color: #363636;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../Piratex.png" />
      </image>
   </ImageView>

    <Label fx:id="welcomeText" textFill="WHITE" />
    <Button onAction="#onHelloButtonClick" prefHeight="25.0" prefWidth="56.0" style="-fx-background-color: #000000;" text="Hello!" textFill="WHITE">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button fx:id="busc" mnemonicParsing="false" onAction="#selectFile" prefHeight="25.0" prefWidth="57.0" style="-fx-background-color: #000000;" text="Buscar" textFill="WHITE">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button fx:id="toca" mnemonicParsing="false" onAction="#playAudio" prefHeight="25.0" prefWidth="58.0" style="-fx-background-color: #000000;" text="Play" textFill="WHITE">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button mnemonicParsing="false" onAction="#pauseAudio" prefHeight="25.0" prefWidth="61.0" style="-fx-background-color: #000000;" text="Pause" textFill="WHITE">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
   <Button fx:id="sai" mnemonicParsing="false" onAction="#sairApp" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #000000;" text="Sair" textFill="WHITE">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor></Button>
</VBox>
